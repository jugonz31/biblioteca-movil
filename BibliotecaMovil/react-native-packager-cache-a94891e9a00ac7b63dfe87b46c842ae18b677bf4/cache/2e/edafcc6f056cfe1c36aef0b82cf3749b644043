  }

  return PropTypes.bool.apply(PropTypes, [props, propName, componentName].concat(rest)) || checker();
};

var ProgressBarAndroid = createReactClass({
  displayName: 'ProgressBarAndroid',
  propTypes: babelHelpers.extends({}, ViewPropTypes, {
